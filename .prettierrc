{
  "overrides": [
    {
      "files": "*.{ts,js}",
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": true,
        "parser": "typescript",
        "trailingComma": "all",
        "arrowParens": "always"
      }
    }
  ]
}
